{
  "vendor_config": {
    "arista": {
        "vendor_name": "arista",
        "session_type": "ssh",
        "prompt_regex": ["[\\w.]+(\\(s\\d+\\))?(\\(config.*\\))?[#>]\\s*"],
        "cli_setup": [ "en", "term len 0" ],
        "shell_prompts": ["\\$"]
    },
    "exr": {
        "vendor_name": "exr",
        "session_type": "ssh",
        "prompt_regex": ["(\\r)?[\\w./:-]+(\\(config.*\\))?[$#]\\s*"],
        "cli_setup": [ "term len 0" ]
    },
    "iosxr": {
        "vendor_name": "iosxr",
        "session_type": "ssh",
        "prompt_regex": ["(\\r)?[\\w./:-]+(\\(config.*\\))?[$#]\\s*"],
        "cli_setup": [ "term len 0" ]
    },
    "juniper": {
        "vendor_name": "juniper",
        "session_type": "ssh",
        "prompt_regex": ["[\\w.@-]+(:.* )?[>#]\\s*"],
        "cli_setup": [
          "set cli screen-length 0",
          "set cli screen-width 1024"
        ],
        "shell_prompts": [ "%" ]
    },
    "nexus": {
        "vendor_name": "nexus",
        "session_type": "ssh",
        "prompt_regex": ["\\s*(\\]\\]>\\]\\]>\\r)?[\\w.]+#\\s*"],
        "cli_setup": [ "term len 0" ],
        "shell_prompts": [ ">" ]
    }
  }
}
